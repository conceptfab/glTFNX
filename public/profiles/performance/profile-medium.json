{
  "id": "profile-medium",
  "name": "Średnia jakość",
  "description": "Zbalansowany profil dla większości urządzeń",
  "category": "profile",
  "renderer": {
    "antialias": false,
    "precision": "mediump",
    "powerPreference": "default",
    "failIfMajorPerformanceCaveat": false,
    "depth": true,
    "stencil": true,
    "premultipliedAlpha": true,
    "preserveDrawingBuffer": true,
    "xrCompatible": false,
    "autoClear": true,
    "logarithmicDepthBuffer": true,
    "toneMapping": "ReinhardToneMapping",
    "toneMappingExposure": 1.0,
    "outputEncoding": "sRGBEncoding",
    "alpha": true,
    "shadowMap": {
      "enabled": true,
      "type": "PCFSoftShadowMap",
      "mapSize": {
        "width": 2048,
        "height": 2048
      },
      "blurSamples": 8,
      "bias": -0.001,
      "radius": 4
    },
    "physicallyCorrectLights": true,
    "pixelRatio": 1.0
  },
  "contactShadows": {
    "enabled": true,
    "quality": "medium",
    "blur": {
      "amount": 2.5,
      "samples": 4
    },
    "darkness": 1.0,
    "opacity": 1.0,
    "resolution": {
      "width": 512,
      "height": 512
    }
  },
  "floor": {
    "enabled": true,
    "quality": "medium",
    "color": "#ffffff",
    "opacity": 1.0,
    "segments": 128,
    "material": {
      "type": "basic",
      "roughness": 0.5,
      "metalness": 0.0
    }
  },
  "floorQuality": {
    "segments": 128
  },
  "postprocessing": {
    "enabled": true,
    "bloom": true,
    "ssao": true,
    "fxaa": true,
    "smaa": false
  },
  "quality": {
    "shadows": "medium",
    "textures": "high",
    "resolution": {
      "width": 1920,
      "height": 1080
    },
    "anisotropy": 8
  }
}
