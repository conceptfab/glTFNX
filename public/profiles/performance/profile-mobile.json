{
  "id": "profile-mobile",
  "name": "Mobilny (SMAA)",
  "description": "Profil zoptymalizowany dla urządzeń mobilnych z SMAA",
  "category": "profile",
  "renderer": {
    "antialias": false,
    "precision": "lowp",
    "powerPreference": "low-power",
    "failIfMajorPerformanceCaveat": true,
    "depth": true,
    "stencil": false,
    "premultipliedAlpha": true,
    "preserveDrawingBuffer": false,
    "xrCompatible": false,
    "autoClear": true,
    "logarithmicDepthBuffer": false,
    "toneMapping": "LinearToneMapping",
    "toneMappingExposure": 1.0,
    "outputEncoding": "sRGBEncoding",
    "alpha": true,
    "shadowMap": {
      "enabled": false,
      "type": "BasicShadowMap",
      "mapSize": {
        "width": 512,
        "height": 512
      },
      "blurSamples": 2,
      "bias": -0.001,
      "radius": 2
    },
    "pixelRatio": 1.0
  },
  "floorQuality": {
    "segments": 32
  },
  "postprocessing": {
    "enabled": true,
    "bloom": false,
    "ssao": false,
    "fxaa": false,
    "smaa": true
  },
  "quality": {
    "shadows": "low",
    "textures": "medium",
    "resolution": {
      "width": 1280,
      "height": 720
    },
    "anisotropy": 1
  }
}
