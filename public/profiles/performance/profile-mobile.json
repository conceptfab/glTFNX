{
  "id": "profile-mobile",
  "name": "Mobilny (SMAA)",
  "description": "Profil zoptymalizowany dla urządzeń mobilnych z SMAA",
  "category": "profile",
  "renderer": {
    "antialias": false,
    "precision": "lowp",
    "powerPreference": "low-power",
    "failIfMajorPerformanceCaveat": true,
    "depth": true,
    "stencil": false,
    "premultipliedAlpha": true,
    "preserveDrawingBuffer": false,
    "xrCompatible": false,
    "autoClear": true,
    "logarithmicDepthBuffer": false,
    "toneMapping": "NoToneMapping",
    "toneMappingExposure": 1.0,
    "outputEncoding": "sRGBEncoding",
    "alpha": true,
    "shadowMap": {
      "enabled": false,
      "type": "BasicShadowMap",
      "mapSize": {
        "width": 512,
        "height": 512
      },
      "blurSamples": 2,
      "bias": -0.001,
      "radius": 2
    },
    "physicallyCorrectLights": false,
    "pixelRatio": 1.0
  },
  "contactShadows": {
    "enabled": false,
    "quality": "low",
    "blur": {
      "amount": 1.5,
      "samples": 2
    },
    "darkness": 1.0,
    "opacity": 1.0,
    "resolution": {
      "width": 256,
      "height": 256
    }
  },
  "floor": {
    "enabled": true,
    "quality": "low",
    "color": "#ffffff",
    "opacity": 1.0,
    "segments": 32,
    "material": {
      "type": "basic",
      "roughness": 0.5,
      "metalness": 0.0
    }
  },
  "floorQuality": {
    "segments": 32
  },
  "postprocessing": {
    "enabled": true,
    "bloom": false,
    "ssao": false,
    "fxaa": false,
    "smaa": true
  },
  "quality": {
    "shadows": "low",
    "textures": "medium",
    "resolution": {
      "width": 1280,
      "height": 720
    },
    "anisotropy": 1
  }
}
